<!DOCTYPE html>
<html>
	<head>
			<title>InventoryManagement</title>
			<%= stylesheet_link_tag :all %>
			<%= javascript_include_tag :all %>
			<!-- <%= csrf_meta_tag %> -->
	</head>
	<body>
		<div class="container">
			<%= render 'layouts/header' %>
			
			<% flash.each do |key, value| %>
				<div class="flash <%= key %>"><%= value %></div>
			<% end %>
			<div class='yield'>
			<%= yield %>
			</div>
		</div>
	</body>
</html>
